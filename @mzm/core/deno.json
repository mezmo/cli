{
  "name": "@mzm/core",
  "tasks": {
    "dev": "deno run --watch main.ts"
  },
  "exports": {
    ".": "./mod.ts",
    "./crypto": "./crypto.ts",
    "./lang": "./lang/mod.ts",
    "./resource": "./resource/mod.ts",
    "./resource/types": "./resource/types.ts",
    "./chrono": "./chrono.vnd.mjs"
  },
  "fmt": {
    "useTabs": false,
    "indentWidth": 2,
    "lineWidth": 110,
    "spaceSurroundingProperties": false,
    "singleQuote": true,
    "semiColons": false
  },
  "imports": {
    "@anitrend/request-client": "jsr:@anitrend/request-client@^0.1.3",
    "@cliffy/command": "jsr:@cliffy/command@^1.0.0-rc.8",
    "@cliffy/prompt": "jsr:@cliffy/prompt@^1.0.0-rc.8",
    "@cliffy/table": "jsr:@cliffy/table@1.0.0-rc.8",
    "@std/assert": "jsr:@std/assert@1",
    "@std/testing": "jsr:@std/testing@^1.0.16",
    "@std/text": "jsr:@std/text@^1.0.16",
    "@std/yaml": "jsr:@std/yaml@^1.0.10"
  }
}
