{
  "name": "@mzm/core",
  "tasks": {
    "dev": "deno run --watch main.ts"
  },
  "exports": {
    ".": "./mod.ts",
    "./crypto": "./crypto.ts",
    "./release": "./release.ts",
    "./lang": "./lang/mod.ts",
    "./resource": "./resource/mod.ts",
    "./resource/types": "./resource/types.ts",
